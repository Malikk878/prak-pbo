#Tugas 1

kotak_bintang = int(input('Input besar kotak: '))
print()
 
for i in range(kotak_bintang):
  for j in range(kotak_bintang):
    print(' *',end='')
  print()

#Tugas 1.2

username = 'informatika'

password = '12345678'

userInput = input("Masukan username: ")

if userInput == username:
    a=input("Password: ")   
    if a == password:
        print("Berhasil login!")
    else:
        print("Username atau password salah coba lagi")
else:
    print("Username atau password salah coba lagi")
